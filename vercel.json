{
  "name": "krantz-dev",
  "version": 2,
  "alias": ["krantz.dev"],
  "public": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Clacks-Overhead",
          "value": "GNU Terry Pratchett"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "same-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": " default-src 'none'; font-src https://cdnjs.cloudflare.com; frame-src https://drive.google.com; img-src 'self'; script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/ "
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=6307200; includeSubDomains; preload"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  },
  "build": {
    "env": {
      "HUGO_VERSION": "0.83.1"
    }
  }
}
